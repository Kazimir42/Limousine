<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=yes, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{% block title %}Limousine{% endblock %}</title>

    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('css/normalize.css') }}">
    <link rel="stylesheet" href="{{ asset('css/app.css') }}">
    <script src="https://unpkg.com/@tabler/core@1.0.0-beta3/dist/js/tabler.min.js"></script>
    <link rel="stylesheet" href="https://unpkg.com/@tabler/core@1.0.0-beta3/dist/css/tabler.min.css">
    <script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>
</head>
<body>
    {# HEADER #}
    {% include('inc/header.html.twig') %}
    {# NAVBAR #}
    {% include('inc/nav.html.twig') %}


    <div class="container-xl">
        <div class="row align-items-center">
            <div class="col-lg-12">
                {# read and display all flash messages #}
                {% for label, messages in app.flashes %}
                    {% for message in messages %}
                        <div class="alert alert-{{ label }}" style="margin-top: 1rem" role="alert">
                            <div class="alert-title">
                                {{ message }}
                            </div>
                        </div>
                    {% endfor %}
                {% endfor %}
            </div>
        </div>
    </div>

    <div class="container-xl">
        <div class="page-header d-print-none">
            <div class="row align-items-center">
                <div class="col">
                    <!-- Page pre-title -->
                    <div class="page-pretitle">
                        {# CAT #}
                        {% block cath1 %}{% endblock %}
                    </div>
                    <h1 class="page-title">
                        {# H1 #}
                        {% block h1 %}{% endblock %}
                    </h1>
                </div>
            </div>
        </div>
    </div>

    <div class="page-body">
        <div class="container-xl">
            <div class="row">
                {# CONTENT #}
                {% block body %}
                {% endblock %}
            </div>
        </div>
    </div>

    {# FOOTER #}
    {% include('inc/footer.html.twig') %}


</body>
</html>